{
  "root": true,
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended", "plugin:react/jsx-runtime"],
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off",
    "react/jsx-uses-react": 1
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "env":{
    "browser": true,
    "node": true
  }
}
